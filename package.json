{
  "name": "testing101",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "wrangler pages deploy .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "postinstall": "patch-package",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --remote",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:seed": "npx tsx scripts/seed.ts --remote",
    "db:seed:local": "npx tsx scripts/seed.ts --local",
    "db:studio": "drizzle-kit studio",
    "db:refresh:local": "rm -rf .wrangler/state && wrangler d1 migrations apply DB --local",
    "cf:typegen": "wrangler types",
    "logs": "wrangler pages deployment tail --project-name=layang-app --format=pretty"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/kit": "^2.20.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/svelte": "^5.2.0",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.0",
    "jsdom": "^26.0.0",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.0",
    "postinstall-postinstall": "^2.1.0",
    "svelte": "^5.53.6",
    "svelte-check": "^4.2.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.0",
    "typescript": "^5.7.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "arctic": "^3.7.0",
    "drizzle-orm": "^0.40.0",
    "kysely": "^0.28.11",
    "kysely-d1": "^0.4.0",
    "lucide-svelte": "^0.475.0",
    "resend": "^4.0.0",
    "zod": "^4.3.6"
  },
  "type": "module"
}
